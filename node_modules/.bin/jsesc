#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/new_vol/Projects/conclave-hackathon/EarlyMind/node_modules/jsesc/bin/node_modules:/mnt/new_vol/Projects/conclave-hackathon/EarlyMind/node_modules/jsesc/node_modules:/mnt/new_vol/Projects/conclave-hackathon/EarlyMind/node_modules:/mnt/new_vol/Projects/conclave-hackathon/node_modules:/mnt/new_vol/Projects/node_modules:/mnt/new_vol/node_modules:/mnt/node_modules:/node_modules:/mnt/new_vol/Projects/conclave-hackathon/EarlyMind/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/new_vol/Projects/conclave-hackathon/EarlyMind/node_modules/jsesc/bin/node_modules:/mnt/new_vol/Projects/conclave-hackathon/EarlyMind/node_modules/jsesc/node_modules:/mnt/new_vol/Projects/conclave-hackathon/EarlyMind/node_modules:/mnt/new_vol/Projects/conclave-hackathon/node_modules:/mnt/new_vol/Projects/node_modules:/mnt/new_vol/node_modules:/mnt/node_modules:/node_modules:/mnt/new_vol/Projects/conclave-hackathon/EarlyMind/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jsesc/bin/jsesc" "$@"
else
  exec node  "$basedir/../jsesc/bin/jsesc" "$@"
fi
